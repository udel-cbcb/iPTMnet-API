{
  "swagger" : "2.0",
  "info" : {
    "description" : "This is an API for iPTMNet providing information about post-translational modifications (PTMs) combined from multiple sources and tools.",
    "version" : "1.0.0",
    "title" : "iPTMNet API",
    "contact" : {
      "email" : "jcowart@udel.edu"
    },
    "license" : {
      "name" : "CC BY-NC-ND 4.0",
      "url" : "https://creativecommons.org/licenses/by-nc-nd/4.0/"
    }
  },
  "host" : "host_url",
  "basePath" : "base_path",
  "schemes" : [ "http_scheme" ],
  "paths" : {
    "/search" : {
      "get" : {
        "summary" : "Search for proteins in the iPTMnet database",
        "operationId" : "searchProteins",
        "description" : "Search for proteins in the iPTMnet database that are relevant to the given search term.",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "in" : "query",
          "name" : "search_term",
          "type" : "string",
          "required" : true,
          "description" : "The term you would like to search"
        }, {
          "in" : "query",
          "name" : "term_type",
          "type" : "string",
          "required" : true,
          "enum" : [ "All", "UniprotID", "Protein/Gene Name", "PMID" ],
          "description" : "The type of search term"
        }, {
          "in" : "query",
          "name" : "ptm_type",
          "type" : "array",
          "items" : {
            "type" : "string"
          },
          "required" : false,
          "description" : "The type of PTM to search for. Possible values are Acetylation, C-Glycosylation, Myristoylation, Ubiquitination, N-Glycosylation, S-Glycosylation, Phosphorylation, S-Nitrosylation, O-Glycosylation, Methylation, Sumoylation"
        }, {
          "in" : "query",
          "name" : "role",
          "type" : "string",
          "required" : true,
          "enum" : [ "Enzyme or Substrate", "Enzyme", "Substrate", "Enzyme and Substrate" ]
        }, {
          "in" : "query",
          "name" : "organism",
          "type" : "array",
          "items" : {
            "type" : "integer"
          },
          "required" : false,
          "description" : "The type of organism to search. The organism is represented by it's taxon ID."
        } ],
        "responses" : {
          "200" : {
            "description" : "A list of result items.",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/search_result_item"
              }
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    },
    "/{id}/info" : {
      "get" : {
        "summary" : "Retrieve top level info about a iPTMNet entry.",
        "operationId" : "infoById",
        "description" : "By passing a IPTMNet identifier, you can retrieve top level info obout that protein entry in iPTMNet.",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "$ref" : "#/parameters/iptmnet_id"
        } ],
        "responses" : {
          "200" : {
            "description" : "an info object.",
            "schema" : {
              "$ref" : "#/definitions/protein_info"
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    },
    "/{id}/msa" : {
      "get" : {
        "summary" : "Retrieve the annotated MSA for the iPTMNet Entry",
        "operationId" : "msaById",
        "description" : "By passing a IPTMNet identifier, you can retrieve the default annotated Multiple Sequence Alignment for the entry",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "$ref" : "#/parameters/iptmnet_id"
        } ],
        "responses" : {
          "400" : {
            "description" : "bad input parameter"
          },
          "501" : {
            "description" : "Not yet implemented. Need to find format."
          }
        }
      }
    },
    "/{id}/substrate" : {
      "get" : {
        "summary" : "Retrieve the list of PTM sites with the given substrate",
        "operationId" : "asSubById",
        "description" : "By passing a IPTMNet identifier, you can retrieve the list of PTM sites and annotations with that given substrate",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "$ref" : "#/parameters/iptmnet_id"
        } ],
        "responses" : {
          "200" : {
            "description" : "Set of tables for the PTM sites for each proteoformof the given protein",
            "schema" : {
              "type" : "array",
              "items" : {
                "type" : "object",
                "properties" : {
                  "form" : {
                    "type" : "string",
                    "example" : "Q15796"
                  },
                  "table" : {
                    "$ref" : "#/definitions/substrate_table"
                  }
                }
              },
              "example" : [ {
                "form" : "Q15796",
                "table" : {
                  "$ref" : "#/definitions/substrate_table/example"
                }
              }, {
                "form" : "Q15796-1",
                "table" : [ {
                  "site" : "T8",
                  "type" : "Phosphorylation",
                  "enzyme" : {
                    "id" : "PR:000026189",
                    "type" : "pro_id",
                    "name" : "hMAPK3/iso:1/Phos:1"
                  },
                  "score" : 2,
                  "sources" : {
                    "PRO" : "http://purl.obolibrary.org/obo/PR_Q15796"
                  },
                  "PMIDs" : [ 12193595 ]
                } ]
              } ]
            }
          },
          "501" : {
            "description" : "Not yet implemented. Need to find format."
          }
        }
      }
    },
    "/{id}/proteoforms" : {
      "get" : {
        "summary" : "Retrieve the list of proteoforms for the iPTMNet Entry",
        "operationId" : "proteoformById",
        "description" : "By passing a IPTMNet identifier, you can retrieve the list of proteoforms for this entry",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "$ref" : "#/parameters/iptmnet_id"
        } ],
        "responses" : {
          "200" : {
            "description" : "A list of proteoforms",
