              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/proteoform"
              }
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    },
    "/{id}/ptmppi" : {
      "get" : {
        "summary" : "Retrieve the list of PTM dependent ppi for the iPTMNet Entry",
        "operationId" : "ptmPpiById",
